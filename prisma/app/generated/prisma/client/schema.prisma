generator client {
  provider = "prisma-client-js"
  output   = "app/generated/prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
  contact  Int
}
